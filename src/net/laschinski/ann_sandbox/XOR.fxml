   <children>
      <LineChart fx:id="chart" layoutY="6.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <CategoryAxis side="LEFT" />
        </yAxis>
      </LineChart>
      <Button layoutX="434.0" layoutY="14.0" mnemonicParsing="false" onAction="#buttonPressed" text="Button" />
   </children>
</AnchorPane>
